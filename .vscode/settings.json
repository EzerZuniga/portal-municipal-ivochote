{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tailwindCSS.includeLanguages": {
    "html": "html"
  },
  "emmet.includeLanguages": {
    "tailwindcss": "html"
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false
}